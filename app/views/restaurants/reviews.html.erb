<p id="notice"><%= notice %></p>

<h1> Reviews of <%= @restaurant.name %></h1>

<tbody>
  <% @reviews.each do |review| %>
  <tr>
    <td><%= review.rating%></td>
    <td><%= review.comment%></td>
  </tr>
  <% end %>
</tbody>

